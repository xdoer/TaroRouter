{
  "miniprogramRoot": "dist/",
  "projectname": "ts-vue",
  "description": "ts-vue-example",
  "appid": "touristappid",
  "setting": {
    "urlCheck": true,
    "es6": false,
    "enhance": false,
    "postcss": false,
    "preloadBackgroundData": false,
